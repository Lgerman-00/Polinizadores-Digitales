{
  "functions": {
    "api/index.py": {
      "runtime": "python3.11",
      "maxDuration": 10
    }
  },
  "routes": [
    { "src": "^/(.*\\.(?:css|js|png|jpg|jpeg|webp|svg|gif|ico|txt|csv))$", "dest": "/$1" },
    { "src": "^/$", "dest": "/api/index.py" },
    { "src": "^/(.*)$", "dest": "/api/index.py" }
  ]
}
